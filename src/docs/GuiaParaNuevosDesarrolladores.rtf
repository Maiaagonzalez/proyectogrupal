\fs28 6. Gu\'eda de desarrollo-ejemplo de una pagina nueva:\fs22\par
import React, \{ useState, useEffect \} from 'react';\par
import \{ BrowserRouter as Router, Routes, Route, Link \} from 'react-router-dom';\par
import \{ Button \} from '@/components/ui/button';\par
import \{ Card, CardContent, CardHeader, CardTitle \} from '@/components/ui/card';\par
// P\'e1gina principal del sistema Limpieza de Aulas\par
function Home() \{\par
  return (\par
    <div className="p-6 text-center space-y-4">\par
      <h1 className="text-3xl font-bold">Sistema de Limpieza de Aulas</h1>\par
      <p>Bienvenido al sistema. Desde aqu\'ed pod\'e9s acceder a la gu\'eda de desarrollo o gestionar aulas.</p>\par
      <div className="flex justify-center gap-4">\par
        <Link to="/guia"><Button>Gu\'eda de desarrollo</Button></Link>\par
        <Link to="/aulas"><Button variant="outline">Gesti\'f3n de Aulas</Button></Link>\par
      </div>\par
    </div>\par
  );\par
\}\par
// Componente de gesti\'f3n de aulas\par
function GestionAulas() \{\par
  const [aulas, setAulas] = useState([\par
    \{ id: 1, nombre: 'Aula 1', estado: 'Limpio' \},\par
    \{ id: 2, nombre: 'Aula 2', estado: 'Sucio' \}\par
  ]);\par
  const [nuevaAula, setNuevaAula] = useState('');\par
  const [estado, setEstado] = useState('Limpio');\par
  const agregarAula = () => \{\par
    if (!nuevaAula.trim()) return;\par
    const id = aulas.length + 1;\par
    setAulas([...aulas, \{ id, nombre: nuevaAula, estado \}]);\par
    setNuevaAula('');\par
  \};\par
  const cambiarEstado = (id) => \{\par
    setAulas(\par
      aulas.map((aula) =>\par
        aula.id === id ? \{ ...aula, estado: aula.estado === 'Limpio' ? 'Sucio' : 'Limpio' \} : aula\par
      )\par
    );\par
  \};\par
  useEffect(() => \{\par
    console.log('Aulas actualizadas:', aulas);\par
  \}, [aulas]);\par
  return (\par
    <div className="p-6 space-y-4">\par
      <h2 className="text-2xl font-bold">Gesti\'f3n de Aulas</h2>\par
      <div className="flex items-center gap-2">\par
        <input\par
          type="text"\par
          placeholder="Nombre del aula"\par
          value=\{nuevaAula\}\par
          onChange=\{(e) => setNuevaAula(e.target.value)\}\par
          className="border p-2 rounded w-1/2"\par
        />\par
        <select\par
          value=\{estado\}\par
          onChange=\{(e) => setEstado(e.target.value)\}\par
          className="border p-2 rounded"\par
        >\par
          <option value="Limpio">Limpio</option>\par
          <option value="Sucio">Sucio</option>\par
        </select>\par
        <Button onClick=\{agregarAula\}>Agregar Aula</Button>\par
      </div>\par
      <ul className="mt-4 space-y-2">\par
        \{aulas.map((aula) => (\par
          <li key=\{aula.id\} className="flex justify-between bg-gray-100 p-2 rounded">\par
            <span>\{aula.nombre\} - Estado: \{aula.estado\}</span>\par
            <Button variant="outline" onClick=\{() => cambiarEstado(aula.id)\}>\par
              Cambiar estado\par
            </Button>\par
          </li>\par
        ))\}\par
      </ul>\par
      <Link to="/"><Button variant="link">Volver al inicio</Button></Link>\par
    </div>\par
  );\par
\}\par
// P\'e1gina de gu\'eda de desarrollo\par
function GuiaDesarrollo() \{\par
  return (\par
    <div className="p-6 space-y-6">\par
      <Card className="shadow-xl">\par
        <CardHeader>\par
          <CardTitle>6. Gu\'eda de desarrollo</CardTitle>\par
        </CardHeader>\par
        <CardContent className="space-y-4">\par
          <section>\par
            <h2 className="font-semibold text-lg">C\'f3mo crear una nueva p\'e1gina o componente</h2>\par
            <p>Crear un archivo nuevo dentro de <code>/src/components</code> o <code>/src/pages</code>. Luego importarlo y usarlo dentro de App.jsx.</p>\par
          </section>\par
          <section>\par
            <h2 className="font-semibold text-lg">C\'f3mo agregar una nueva aula o modificar el estado</h2>\par
            <p>Pod\'e9s agregar un componente que use <code>useState</code> para manejar las aulas y un bot\'f3n que cambie su estado de Limpio a Sucio.</p>\par
          </section>\par
          <section>\par
            <h2 className="font-semibold text-lg">Buenas pr\'e1cticas</h2>\par
            <ul className="list-disc ml-6">\par
              <li>Usar nombres descriptivos para variables y componentes.</li>\par
              <li>Organizar carpetas seg\'fan funcionalidad.</li>\par
              <li>Reutilizar componentes cuando sea posible.</li>\par
              <li>Usar clases de Tailwind para estilos consistentes.</li>\par
            </ul>\par
          </section>\par
          <section>\par
            <h2 className="font-semibold text-lg">Uso de useState, useEffect y props (React)</h2>\par
            <p>\par
              <code>useState</code> permite manejar estados locales, como el listado de aulas o el texto de entrada.<br />\par
              <code>useEffect</code> se utiliza para ejecutar c\'f3digo cuando cambia un estado o al montar el componente.<br />\par
              Las <code>props</code> sirven para pasar datos entre componentes.\par
            </p>\par
          </section>\par
          <Link to="/"><Button variant="link">Volver al inicio</Button></Link>\par
        </CardContent>\par
      </Card>\par
    </div>\par
  );\par
\}\par
// Aplicaci\'f3n principal con rutas\par
export default function App() \{\par
  return (\par
    <Router>\par
      <Routes>\par
        <Route path="/" element=\{<Home />\} />\par
        <Route path="/aulas" element=\{<GestionAulas />\} />\par
        <Route path="/guia" element=\{<GuiaDesarrollo />\} />\par
      </Routes>\par
    </Router>\par
  );\par
\}\par
